<div id="newsfeed" class="feed">
	<div class="headline-wrap">

	<a href="https://medium.com/browse/top" class="link-wrap">
		<h1 class="headline"> Top Stories on Medium </h1>
	</a>
	</div>

	{{#each results}}
	<div class="feed-item box">
		{{!-- <div class="text-section"> --}}
			<a href="{{this.link}}" class="link-wrap title-wrap">
				<h2 class="article-title"> {{this.title}} </h2>
			</a>
			<div class="meta-wrap">
				<a href="{{this.author_profile}}" class="author">{{this.author}}</a>
				<p class="publication_date">Published on: {{this.date}}</p>
			</div>
		{{!-- </div> --}}

		{{!-- <div class="thumbnail-section">
			<a href="{{this.link}}" class="link-wrap thumbnail-wrap">
				<img src="{{this.thumbnail}}" alt="{{this.title}}">
			</a>
		</div> --}}

	</div>
	{{/each}}
</div>